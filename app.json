{
  "expo": {
    "name": "mckingapp",
    "slug": "mckingapp",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/burgerKolossos.png",
    "scheme": "mckingapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "backgroundColor": "#E6F4FE",
        "foregroundImage": "./assets/images/burgerKolossos.png",
        "backgroundImage": "./assets/images/burgerKolossos.png",
        "monochromeImage": "./assets/images/burgerKolossos.png"
      },
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false,
      "package": "com.ximench.mckingapp",
      "googleServicesFile": "./google-services.json"
    },
    "web": {
      "output": "static",
      "favicon": "./assets/images/burgerKolossos.png"
    },
    "plugins": [
      "expo-router",
      "expo-updates",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/burgerKolossos.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff",
          "dark": {
            "backgroundColor": "#000000"
          }
        }
      ],
      [
        "expo-notifications",
        {
          "icon": "./assets/images/burgerKolossos.png",
          "color": "#bd93f9"
        }
      ]
    ],
    "updates": {
      "url": "https://u.expo.dev/6483cf7f-be2c-46c6-b8dd-5987799b3930",
      "enabled": true
    },
    "runtimeVersion": {
      "policy": "appVersion"
    },
    "experiments": {
      "typedRoutes": true,
      "reactCompiler": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "6483cf7f-be2c-46c6-b8dd-5987799b3930"
      }
    }
  }
}
